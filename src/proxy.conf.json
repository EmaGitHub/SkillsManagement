{
  "/public/**": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug"
  },
  "/skill/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api/v1" : "/skill/*"
    }
  },
  "/user/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api/v1" : "/user/*"
    }
  },
  "/competence/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api/v1" : "/competence/*"
    }
  }
}